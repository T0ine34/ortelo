{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": [
            "./"
        ],
        "exclude": [
            "./node_modules/",
            "./database",
            "./json_structures",
            "./game",
            "./.vscode",
            "./.git",
            "./public/bootstrap",
            "./docs"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": false,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts":{
        "destination": "./docs/",
        "recurse": true,
        "encoding": "utf8",
        "template" : "node_modules/clean-jsdoc-theme"
    }
}