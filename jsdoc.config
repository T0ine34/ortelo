{
    "recurseDepth": 10,
    "source": {
        "include": [
            "./"
        ],
        "exclude": [
            "./node_modules/",
            "./database",
            "./json_structures",
            "./game",
            "./.vscode",
            "./.git",
            "./public/bootstrap",
            "./docs"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": ["category"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "search": true,
        "better-docs": {
            "title" : "Documentation of S3.A.01 G2 project"
        }
    },
    "plugins": [
        "node_modules/better-docs/category"
    ],
    "opts":{
        "destination": "./docs/",
        "recurse": true,
        "encoding": "utf8",
        "readme": "./README.md",
        "template" : "node_modules/better-docs"
    }
}